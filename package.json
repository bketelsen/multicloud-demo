{
  "name": "multicloud-demo",
  "version": "1.0.0",
  "description": "Azure Functions sample for the Serverless framework",
  "scripts": {
    "test": "jest",
    "test:integration": "npm run test -- --testRegex=test\\.integration\\.js",
    "start": "npm run start:azure",
    "start:azure": "sls offline --config serverless-azure.yml",
    "start:aws": "sls offline --config serverless-aws.yml --httpsProtocol ./cert start",
    "pack": "webpack",
    "deploy": "npm run deploy:azure && npm run deploy:aws",
    "deploy:aws": "sls deploy --config serverless-aws.yml",
    "deploy:azure": "sls deploy --config serverless-azure.yml",
    "generate:spn": "bash ./scripts/generate-service-principal.sh",
    "dotenv": "set -o allexport; source .env; set +o allexport",
    "update:multicloud": "npm install @multicloud/sls-core@beta @multicloud/sls-azure@beta @multicloud/sls-aws@beta @multicloud/sls-711@beta --save"
  },
  "keywords": [
    "azure",
    "serverless"
  ],
  "dependencies": {
    "@multicloud/sls-aws": "^0.1.1-0",
    "@multicloud/sls-azure": "^0.1.1-0",
    "@multicloud/sls-core": "^0.1.1-0",
    "shortid": "^2.2.14"
  },
  "devDependencies": {
    "jest": "^24.8.0",
    "serverless-azure-functions": "^1.0.0-11",
    "serverless-offline": "^5.5.0",
    "serverless-webpack": "^5.3.1",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.3"
  }
}
